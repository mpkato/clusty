<%- model_class = Project -%>
<div class="page-header">
  <h1><%= @project.label %></h1>
  <%= link_to 'Download',  project_path(format: 'tsv') %>
</div>
<div class="panel panel-default">
  <div id="field" class="panel-body" 
    data-id="<%= @project.id %>"
    data-url="<%= project_clusters_path(@project) %>"
  >
  </div>
</div>

<%= link_to t('.back', :default => t("helpers.links.back")),
              projects_path, :class => 'btn btn-default'  %>
